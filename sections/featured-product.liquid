{%- liquid
  assign section_color = section.settings.color_scheme | default: 'primary'
  assign product = all_products[section.settings.product]
  assign current_variant = product.selected_or_first_available_variant
  assign featured_media = product.featured_media

  assign product_form_id = 'product-form-' | append: section.id

  assign has_sticky_buy_buttons = false
  assign enable_image_zoom = false
-%}

{% unless request.design_mode %}
  <script src="{{ 'vendor-product.bundle.min.js' | asset_url }}" type="module"></script>
  <script src="{{ 'product.js' | asset_url }}" type="module"></script>
{% endunless %}

<section
  class="{% unless section.settings.remove_top_padding %}pt-section{% endunless %} {% unless section.settings.remove_bottom_padding %}pb-section{% endunless %} text-scheme-text bg-scheme-background {% if section.settings.disable_color_transition %}disable-section-color-transition disable-section-color-transition-source{% endif %}"
  data-color-scheme="{{ section_color }}"
  {% if product != blank %}
    x-data="
      Theme_Product({
        product: {{ product | json | escape }},
        variant: {{ current_variant | json | escape }},
        featuredMediaID: {{ featured_media.id }}
      })"
    data-product-section
  {% endif %}
>
  <div class="max-w-site mx-auto px-5 lg:px-10">
    <div class="lg:w-7/8 mx-auto">
      <div class="lg:grid lg:grid-cols-2 lg:gap-grid-gutter">
        <div class="w-full lg:w-5/6 lg:ml-auto">



          {% comment %}
            Product media
          {% endcomment %}
          {% if product and product.media.size > 0 %}
            <div
              id="cascade-slideshow-mobile-{{ section.id }}"
              class="splide splide--product"
              data-first-media="{{ featured_media.id }}"
              x-cloak
            >
              <div class="splide__track -mx-5 mb-4">
                <ul class="w-full splide__list" role="list">
                  {% for media in product.media %}
                    {% liquid
                      if media.id == featured_media.id
                        assign shown = true
                        assign loading = 'eager'
                      else
                        assign shown = true
                        assign loading = 'lazy'
                      endif
                    %}
                    <li
                      class="block px-5 w-10/12 md:w-full splide__slide"
                      data-media-id="{{ media.id }}"
                    >
                      {% if enable_image_zoom and media.media_type == 'image' %}
                        <a
                          href="{{ media.url }}"
                          role="button"
                          class="cursor-zoom-in"
                          @click.prevent="openZoom({{ media.id }})"
                        >
                      {% endif %}
                      {% render 'media',
                        media: media,
                        sizes: '100vw, (min-width: 768px) 50vw',
                        shown: shown,
                        always_shown: true,
                        loading: loading,
                      %}
                      {% if enable_image_zoom and media.media_type == 'image' %}
                        </a>
                      {% endif %}
                    </li>
                  {% endfor %}
                </ul>
              </div>
              <div class="splide__arrows lg:col-span-2">
                <button class="splide__arrow splide__arrow--prev mr-1 p-1">
                  <span class="sr-only">{{ 'sections.slideshow.previous_slide' | t }}</span>
                  <div class="w-8 h-8">
                    {% render 'icon-previous' %}
                  </div>
                </button>
                <button class="splide__arrow splide__arrow--next p-1">
                  <span class="sr-only">{{ 'sections.slideshow.next_slide' | t }}</span>
                  <div class="w-8 h-8">
                    {% render 'icon-next' %}
                  </div>
                </button>
              </div>
            </div>
            {% unless request.design_mode %}
              <script src="{{ 'modules-slideshow.bundle.min.js' | asset_url }}" type="module"></script>
            {% endunless %}
            {{ 'modules-slideshow.bundle.css' | asset_url | stylesheet_tag }}
          {% else %}
            {{ 'product-1' | placeholder_svg_tag: 'bg-scheme-placeholder-background text-scheme-placeholder-foreground fill-current' }}
          {% endif %}

        </div>
        <div>
          <section>
          {% comment %}
            Product blocks
          {% endcomment %}
         {% comment %} Product blocks {% endcomment %}
<div class="xl:pt-10 lg:pl-10 xl:pl-20">
  <div class="mt-10" data-product-form-container>
    {% assign block_base_classes = 'max-w-sm px-5 lg:px-10' %}
    {% for block in section.blocks %}
      {% if block.type == 'title' %}
        <div class="{{ block_base_classes }}" {{ block.shopify_attributes }}>
          {% if product != blank %}
            <!-- Your existing product title code -->
          {% else %}
            <!-- Your existing alternative content code -->
          {% endif %}
        </div>
      {% elsif block.type == 'price' %}
        <!-- Your existing price block code -->
      {% elsif block.type == 'variant_picker' %}
        {% if block.settings.hide_option_labels %}
          {% assign label_class = 'sr-only' %}
        {% else %}
          {% assign label_class = 'block font-secondary text-scheme-meta text-sm my-2' %}
        {% endif %}
        <!-- Your existing variant picker code using label_class -->
      {% elsif block.type == 'quantity_selector' %}
        <!-- Your existing quantity selector code -->
      {% elsif block.type == 'buy_buttons' %}
        <!-- Your existing buy buttons code -->
      {% elsif block.type == 'star_rating' %}
        <!-- Your existing star rating code -->
      {% elsif block.type == 'rich_text' %}
        <!-- Your existing rich text code -->
      {% elsif block.type == 'text' %}
        <!-- Your existing text code -->
      {% elsif block.type == 'description' %}
        <!-- Your existing description code -->
      {% elsif block.type == 'supporting_links' %}
        <!-- Your existing supporting links code -->
      {% elsif block.type == 'popup' %}
        <!-- Your existing popup code -->
      {% elsif block.type == 'liquid' %}
        <!-- Your existing liquid block code -->
      {% elsif block.type == 'collapsible_tabs' %}
        <!-- Your existing collapsible tabs code -->
      {% elsif block.type == 'icon_list' %}
        <!-- Your existing icon list code -->
      {% elsif block.type == 'share' %}
        <!-- Your existing share code -->
      {% elsif block.type == '@app' %}
        <!-- Your existing app block code -->
      {% else %}
        <!-- Add any other custom block types here -->
      {% endif %}
    {% endfor %}
  </div>
</div>
</section>

