{% liquid
  assign section_color = section.settings.color_scheme
  assign desktop_layout = section.settings.desktop_layout

  assign enable_slideshow = section.settings.enable_slideshow
  assign desktop_featured_media_width = section.settings.desktop_featured_media_width

  assign current_variant = product.selected_or_first_available_variant
  assign featured_media = current_variant.featured_media | default: product.featured_media

  assign sequence = section.settings.sequence

  assign models = product.media | where: 'media_type', 'model'

  for media in product.media
    if media.media_type != 'image'
      assign has_interactive_media = true
      break
    endif
  endfor

  assign enable_image_zoom = section.settings.enable_image_zoom

  assign popup_blocks = section.blocks | where: 'type', 'popup'
%}

{% unless request.design_mode %}
  <script src="{{ 'vendor-product.bundle.min.js' | asset_url }}" type="module"></script>
  <script src="{{ 'product.js' | asset_url }}" type="module"></script>
{% endunless %}

{% style %}
  #{{ section.id }} {
  {% unless desktop_layout == 'product_form_right' %}
    --cascade-product-first-col-width: var(--cascade-product-form-width);
    --cascade-product-second-col-width: var(--cascade-product-cascade-width);
  {% else %}
    --cascade-product-first-col-width: var(--cascade-product-cascade-width);
    --cascade-product-second-col-width: var(--cascade-product-form-width);
  {% endunless %}
  }
{% endstyle %}

{%- if featured_media != null and product.media.size > 0 -%}
  {%- capture featured_media_block -%}
    {% if product.media.size == 1 %}
      <div>
        {% if enable_image_zoom and featured_media.media_type == 'image' %}
          <a
            href="{{ media.url }}"
            role="button"
            class="cursor-zoom-in"
            @click.prevent="openZoom({{ media.id }})"
          >
        {% endif %}
        {% render 'media',
           media: featured_media,
           sizes: '100vw, (min-width: 768px) 50vw',
           shown: true,
           always_shown: true,
           loading: 'eager',
        %}
        {% if enable_image_zoom and featured_media.media_type == 'image' %}
          </a>
        {% endif %}
      </div>
    {% else %}
      {% unless enable_slideshow %}
        <ul>
          {% for media in product.media %}
            {%- liquid
              if media.id == featured_media.id
                assign shown = true
                assign loading = 'eager'
              else
                assign shown = false
                assign loading = 'lazy'
              endif
            -%}
            <li class="md:col-span-6">
              {% if enable_image_zoom and media.media_type == 'image' %}
                <a
                  href="{{ media.url }}"
                  role="button"
                  class="cursor-zoom-in"
                  @click.prevent="openZoom({{ media.id }})"
                >
              {% endif %}
              {% render 'media',
                media: media,
                sizes: '90vw, (min-width: 768px) 50vw',
                shown: shown,
                loading: loading,
                hidden_class: 'hidden'
              %}
              {% if enable_image_zoom and media.media_type == 'image' %}
                </a>
              {% endif %}
            </li>
          {% endfor %}
        </ul>
        {% assign first_3d_model = product.media | where: 'media_type', 'model' | first %}
          {%- if first_3d_model -%}
            <div class="mb-4">
              <button
                aria-label="{{ 'products.media.view_in_your_space_label' | t }}"
                class="w-full px-5 py-3 bg-scheme-text/5 text-scheme-text/80"
                data-shopify-xr
                data-shopify-model3d-id="{{ first_3d_model.id }}"
                data-shopify-title="{{ product.title }}"
                data-shopify-xr-hidden
              >
                <span class="inline-block w-6 -mt-1 align-middle">{% render 'icon-model' %}</span><span class="text-sm">{{ 'products.media.view_in_your_space' | t }}</span>
              </button>
            </div>
          {%- endif -%}
      {% else %}
        <div
          id="cascade-slideshow-mobile-{{ section.id }}"
          class="splide splide--product"
          data-drag-disabled="{{ has_interactive_media }}"
          data-first-media="{{ featured_media.id }}"
          x-cloak
        >
          <div class="splide__track -mx-5 mb-4">
            <ul class="w-full splide__list" role="list">
              {% for media in product.media %}
                {% liquid
                  if media.id == featured_media.id
                    assign shown = true
                    assign loading = 'eager'
                  else
                    assign shown = true
                    assign loading = 'lazy'
                  endif
                %}
                <li
                  class="block px-5 w-10/12 md:w-full splide__slide"
                  data-media-id="{{ media.id }}"
                >
                  {% if enable_image_zoom and media.media_type == 'image' %}
                    <a
                      href="{{ media.url }}"
                      role="button"
                      class="cursor-zoom-in"
                      @click.prevent="openZoom({{ media.id }})"
                    >
                  {% endif %}
                  {% render 'media',
                    media: media,
                    sizes: '100vw, (min-width: 768px) 50vw',
                    shown: shown,
                    always_shown: true,
                    loading: loading,
                  %}
                  {% if enable_image_zoom and media.media_type == 'image' %}
                    </a>
                  {% endif %}
                </li>
              {% endfor %}
            </ul>
          </div>
          {% assign first_3d_model = product.media | where: 'media_type', 'model' | first %}
          {%- if first_3d_model -%}
            <div class="col-span-2 mb-4">
              <button
                aria-label="{{ 'products.media.view_in_your_space_label' | t }}"
                class="w-full px-5 py-3 bg-scheme-text/5 text-scheme-text/80"
                data-shopify-xr
                data-shopify-model3d-id="{{ first_3d_model.id }}"
                data-shopify-title="{{ product.title }}"
                data-shopify-xr-hidden
              >
                <span class="inline-block w-6 -mt-1 align-middle">{% render 'icon-model' %}</span><span class="text-sm">{{ 'products.media.view_in_your_space' | t }}</span>
              </button>
            </div>
          {%- endif -%}
          <div class="splide__arrows lg:col-span-2">
            <button class="splide__arrow splide__arrow--prev mr-1 p-1">
              <span class="sr-only">{{ 'sections.slideshow.previous_slide' | t }}</span>
              <div class="w-8 h-8">
                {% render 'icon-previous' %}
              </div>
            </button>
            <button class="splide__arrow splide__arrow--next p-1">
              <span class="sr-only">{{ 'sections.slideshow.next_slide' | t }}</span>
              <div class="w-8 h-8">
                {% render 'icon-next' %}
              </div>
            </button>
          </div>
        </div>
        {% unless request.design_mode %}
          <script src="{{ 'modules-slideshow.bundle.min.js' | asset_url }}" type="module"></script>
        {% endunless %}
        {{ 'modules-slideshow.bundle.css' | asset_url | stylesheet_tag }}
      {% endunless %}
    {% endif %}
  {%- endcapture -%}
  {%- capture featured_media_cascade_block -%}
    <div data-featured-media-cascade-block data-disable-variance data-cascade-featured-media  {% unless desktop_layout == 'full_width' %}data-cascade-hide-on-mobile{% endunless %}>
    {%- assign cascade_block = featured_media_block | replace: 'cascade-slideshow-mobile-', 'cascade-slideshow-desktop-' %}
    {{ cascade_block }}
    </div>
  {%- endcapture -%}
{%- endif -%}

{%- capture product_form -%}
  <div data-product-form-container data-disable-variance data-cascade-product-form>
    <div class="max-w-sm mx-auto px-10">
    {% liquid
      assign product_form_id = "ProductForm-" | append: section.id

      assign block_base_classes = ''

      for block in section.blocks
        case block.type
          when '@app'
            render 'product-block-app', block: block, block_base_classes: block_base_classes

          when 'title'
            render 'product-block-title', product: product, block: block, block_base_classes: block_base_classes

          when 'price'
            assign price_class = block_base_classes | append: ' my-3'
            render 'product-form-component-price', product: product, block: block, class: price_class

          when 'variant_picker'
            if block.settings.hide_option_labels
              assign label_class = 'sr-only'
            else
              assign label_class = 'block font-secondary my-2'
            endif
            render 'product-form-component-variant-picker', product: product, block: block, label_class: label_class, class: block_base_classes, product_form_id: product_form_id

          when 'quantity_selector'
            if block.settings.hide_label
              assign label_class = 'sr-only'
            else
              assign label_class = 'block font-secondary my-2'
            endif

            render 'product-block-quantity', product_form_id: product_form_id, label_class: label_class, block: block, block_base_classes: block_base_classes

          when 'buy_buttons'
            render 'product-block-buy-buttons', product: product, product_form_id: product_form_id, label_class: label_class, current_variant: current_variant, block: block, block_base_classes: block_base_classes

          when 'complementary'
            render 'product-block-complementary', product: product, block: block, block_base_classes: block_base_classes

          else
            if block.settings.section_area == 'form'
              case block.type
                when 'star_rating'
                  render 'product-block-star-rating', product: product, block: block, block_base_classes: block_base_classes

                when 'text'
                  render 'product-block-text', block: block, block_base_classes: block_base_classes

                when 'rich_text'
                  render 'product-block-rich-text', block: block, block_base_classes: block_base_classes

                when 'description'
                  render 'product-block-description', product: product, block: block, block_base_classes: block_base_classes

                when 'supporting_links'
                  render 'product-block-supporting-links', block: block, block_base_classes: block_base_classes

                when 'popup'
                    render 'aloha-popup', block: block, block_base_classes: block_base_classes

                when 'liquid'
                  render 'product-block-liquid', block: block, block_base_classes: block_base_classes

                when 'collapsible_tabs'
                  render 'product-block-collapsible-tabs', block: block, block_base_classes: block_base_classes

                when 'icon_list'
                  render 'product-block-icon-list', block: block, block_base_classes: block_base_classes

                when 'share'
                  render 'product-block-share', product: product, block: block, block_base_classes: block_base_classes

              endcase
            endif
        endcase
      endfor
    %}
    </div>
  </div>
{%- endcapture -%}

<section
  id="{{ section.id }}"
  x-data="
    Theme_Product({
      product: {{ product | json | escape }},
      variant: {{ current_variant | json | escape }},
      featuredMediaID: {% if featured_media %}{{ featured_media.id }}{% else %}null{% endif %}
    })
  "
  class="bg-scheme-background text-scheme-text"
  data-color-scheme="{{ section_color }}"
  data-product-section
  @product-url-update.stop="window.history.replaceState({ path: $event.detail.url }, '', $event.detail.url);"
>
  <div class="md:grid md:grid-cols-[1fr_var(--cascade-product-first-col-width)_var(--cascade-product-second-col-width)_1fr]">
    {% comment %}
      Mobile featured media + product form and column on desktop if enabled
    {% endcomment %}
    <div class="col-span-2 xl:pt-10 text-scheme-text bg-scheme-background md:min-h-full {% if desktop_layout == 'product_form_right' %} col-end-last {% endif %} {% if desktop_layout == 'full_width' %} hidden {% else %} pb-10 md:pb-0 {% endif %}">
      <div class="md:max-w-[var(--cascade-product-form-width)] {% if desktop_layout == 'product_form_right' %} md:mr-auto {% else %} md:ml-auto {% endif %} md:mt-10 md:sticky md:top-header-height  pt-10 md:pb-10">
        <div class="md:hidden mb-5 md:mb-10 px-5 pt-[calc(var(--spacing-section)_-_2.5rem)]">
          {{ featured_media_block }}
        </div>
        {% unless desktop_layout == 'full_width' %}
          {{ product_form }}
        {% endunless %}
      </div>
    </div>

    {% comment %}
      Cascading content
    {% endcomment %}
    <div class="px-5 md:px-0 {% if desktop_layout == 'full_width' %}col-span-full{% else %}col-span-2{% endif %} {% if desktop_layout == 'product_form_right' %} col-start-1 row-start-1 {% endif %} text-scheme-text bg-scheme-background pt-0 pb-section md:py-section md:min-h-full cascade-block-container">
      {%- liquid
        if section.settings.variance == 'global'
          assign variance = settings.variance
        else
          assign variance = section.settings.variance
        endif

        if section.settings.spacing == 'global'
          assign spacing = settings.cascade_item_spacing
        else
          assign spacing = section.settings.spacing
        endif

        assign sizes = '50vw'

        assign cascade_block_base_classes = 'ml-[inherit] mr-[inherit] px-5 lg:px-10'

        assign media_cursor = 0

        if section.settings.skip_featured_media
          assign skip_featured_media = true
          assign slice_point = featured_media.position | minus: 1
          assign first_part = product.media | slice: 0, slice_point
          assign second_part = product.media | slice: featured_media.position, product.media.size
          assign media_array = first_part | concat: second_part
        else
          assign media_array = product.media
        endif

        assign full_width_item_header = '||__cascade-item:full__||'
        assign item_separator = '||__cascade-item-end__||'

        capture cascade_items
          if featured_media != null and product.media.size > 0
            if desktop_featured_media_width == 'full'
              echo full_width_item_header
            endif
            echo featured_media_cascade_block
            echo item_separator
          endif
          if desktop_layout == 'full_width'
            echo product_form
            echo item_separator
          endif
          for block in section.blocks
            case block.type
              when 'media_range', 'single_product_media'
                assign not_first_of_media_items = false

                if block.settings.media_range contains '/'
                  assign range = block.settings.media_range | split: '/' | first | plus: 0
                  assign divisor = block.settings.media_range | split: '/' | last | plus: 0.00

                  assign total_media = media_array.size

                  comment
                    ceil makes it greedier, so ranges on unevenly divisible amounts of media will be higher (resulting in earlier failure and fewer images in the last range)
                  endcomment

                  assign range_size = total_media | divided_by: divisor | ceil

                  assign previous_range = range | minus: 1

                  comment
                    first index: range_size * (range - 1) + 1
                    last index: range_size * (range - 1) + range_size

                    subtract 1 for zero-based media array if featured media included
                  endcomment

                  assign first_media_index = range_size | times: previous_range | plus: 1 | minus: 1
                  assign last_media_index = range_size | times: previous_range | plus: range_size | minus: 1
                else
                  if block.type == 'single_product_media'
                    assign range = 0

                    assign first_media_index = media_cursor
                    assign last_media_index = media_cursor | plus: range
                  elsif block.settings.media_range == 'all'
                    assign total_remaining_media = media_array.size | minus: media_cursor

                    assign first_media_index = media_cursor
                    assign last_media_index = media_cursor | plus: total_remaining_media
                  endif
                endif

                for i in (first_media_index..last_media_index)
                  if media_array[i]
                    assign media = media_array[i]
                    if block.type == 'single_product_media' and block.settings.full_width
                      echo full_width_item_header
                    endif
                    echo '<div'
                    unless not_first_of_media_items
                      echo block.shopify_attributes | prepend: ' '
                      assign not_first_of_media_items = true
                      assign loading = 'eager'
                    else
                      assign loading = 'lazy'
                    endunless
                    echo '>'
                    if enable_image_zoom and media.media_type == 'image'
                      echo '<a'
                      echo ' href="' | append: media.url | append: '"'
                      echo ' role="button"'
                      echo ' class="cursor-zoom-in"'
                      echo ' @click.prevent="openZoom(' | append: media.id | append: ')"'
                      echo '>'
                    endif
                    echo '<div data-scale-reveal>'
                    render 'media', media: media, sizes: sizes, shown: true, scroll_to: false, always_shown: true, loading: loading, media_item: true
                    echo '</div>'
                    if enable_image_zoom and media.media_type == 'image'
                      echo '</a>'
                    endif
                    echo '</div>'
                    echo item_separator

                    if block.type != 'media_range'
                      assign media_cursor = i | plus: 1
                    endif
                  else
                    break
                  endif
                endfor
              else
                if block.settings.section_area == 'cascade'
                  case block.type
                    when 'star_rating'
                      render 'product-block-star-rating', product: product, block: block, block_base_classes: cascade_block_base_classes
                    when 'text'
                      render 'product-block-text', block: block, block_base_classes: cascade_block_base_classes
                    when 'rich_text'
                      render 'product-block-rich-text', block: block, block_base_classes: cascade_block_base_classes
                    when 'description'
                      if product.description != blank
                        if block.settings.full_width
                          echo full_width_item_header
                        endif
                        render 'product-block-description', product: product, block: block, block_base_classes: cascade_block_base_classes
                      else
                        continue
                      endif
                    when 'supporting_links'
                      render 'product-block-supporting-links', block: block, block_base_classes: cascade_block_base_classes
                    when 'popup'
                      render 'popup', block: block, block_base_classes: cascade_block_base_classes
                    when 'liquid'
                      render 'product-block-liquid', block: block, block_base_classes: block_base_classes
                    when 'collapsible_tabs'
                      render 'product-block-collapsible-tabs', block: block, block_base_classes: cascade_block_base_classes
                    when 'icon_list'
                      render 'product-block-icon-list', block: block, block_base_classes: cascade_block_base_classes
                    when 'share'
                      render 'product-block-share', product: product, block: block, block_base_classes: cascade_block_base_classes
                  endcase
                  echo item_separator
                endif
            endcase
          endfor
        endcapture

        render 'cascade', content_items: cascade_items, variance: variance, spacing: spacing, first_block_position: section.settings.first_block_position, desktop_layout: section.settings.cascade_desktop_layout, enable_cascade_on_mobile: section.settings.enable_cascade_on_mobile, skip_featured_media: skip_featured_media, featured_media_id: featured_media.id, grid_class: 'grid md:grid-cols-2 md:gap-x-grid-gutter md:gap-y-0 md:w-7/8 mx-auto', single_container_class: 'md:w-7/8 mx-auto', sequence: sequence, cascade_product_desktop_layout: desktop_layout

        for block in popup_blocks
          render 'product-popup-block-modal', section_id: section.id, block: block
        endfor
      -%}
    </div>
  </div>
</section>

{% if models.size %}
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
  <script>
    window.ProductModel = {
      loadShopifyXR() {
        Shopify.loadFeatures([
          {
            name: 'shopify-xr',
            version: '1.0',
            onLoad: this.setupShopifyXR.bind(this),
          },
        ]);
      },
      setupShopifyXR(errors) {
        if (errors) return;
        if (!window.ShopifyXR) {
          document.addEventListener('shopify_xr_initialized', () =>
            this.setupShopifyXR()
          );
          return;
        }
        document.querySelectorAll('[id^="ModelJson-"]').forEach((modelJSON) => {
          window.ShopifyXR.addModels(JSON.parse(modelJSON.textContent));
          modelJSON.remove();
        });
        window.ShopifyXR.setupXRElements();
      },
    };
    window.addEventListener('DOMContentLoaded', () => {
      if (Shopify.designMode) {
        document.querySelectorAll("[data-shopify-xr-hidden]").forEach(element => element.classList.add('hidden'));
      }
      if (window.ProductModel) window.ProductModel.loadShopifyXR();
    });
  </script>
{% endif %}

{% if enable_image_zoom %}
  {% liquid
    assign modal_id = 'image-zoom-' | append: section.id
    assign zoom_section_url = product.url | append: '?section_id=main-product-zoom'
  %}
  <div
    x-data
    x-init="initTeleport($root)"
    x-cloak
  >
    <template data-should-teleport="#modal-slot">
      <template x-if="$store.modals.modal.contents === '{{ modal_id }}'">
        <div class="text-scheme-text bg-scheme-background">
          <button @click.prevent="$store.modals.close('{{ modal_id }}')" class="block ml-auto fixed mt-5 pt-4 right-10">
            <span class="sr-only">{{ 'general.accessibility.close_modal' | t }}</span>
            <span class="inline-block w-6 h-6 drop-shadow-[0.06rem_0.12rem_0_rgb(var(--color-scheme-background))]">
              {%- render 'icon-close' -%}
            </span>
          </button>
          <div
            id="{{ modal_id }}"
            data-modal-label="{{ product.title }}, {{ 'products.media.media' | t }}"
            x-html="
              $fetchedSection(
                '{{ zoom_section_url }}',
                '.product-zoom'
              )
            "
          ></div>
        </div>
      </template>
    </template>
  </div>
  {% unless request.design_mode %}
    <script src="{{ 'zoom.js' | asset_url }}" type="module"></script>
  {% endunless %}
{% endif %}

{% schema %}
{
  "name": "Product",
  "settings": [
    {
      "type": "header",
      "content": "Layout"
    },
    {
      "type": "select",
      "id": "desktop_layout",
      "label": "Desktop layout",
      "options": [
        {
          "value": "product_form_left",
          "label": "Product form on left"
        },
        {
          "value": "product_form_right",
          "label": "Product form on right"
        },
        {
          "value": "full_width",
          "label": "Full width"
        }
      ]
    },
    {
      "type": "header",
      "content": "Featured media"
    },
    {
      "type": "radio",
      "id": "desktop_featured_media_width",
      "default": "half",
      "options": [
        {
          "value": "half",
          "label": "Half width"
        },
        {
          "value": "full",
          "label": "Full width"
        }
      ],
      "label": "Width on desktop"
    },
    {
      "type": "checkbox",
      "id": "enable_slideshow",
      "label": "Enable slideshow",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "skip_featured_media",
      "label": "Skip featured media in cascading content",
      "default": true
    },
    {
      "type": "header",
      "content": "Cascading content"
    },
    {
      "type": "radio",
      "id": "cascade_desktop_layout",
      "label": "Desktop layout",
      "default": "two_columns",
      "options": [
        {
          "value": "single_column",
          "label": "Single column"
        },
        {
          "value": "two_columns",
          "label": "Two columns"
        }
      ]
    },
    {
      "type": "radio",
      "id": "first_block_position",
      "label": "First block position",
      "options": [
        {
          "value": "left",
          "label": "Top left"
        },
        {
          "value": "right",
          "label": "Top right"
        }
      ]
    },
    {
      "type": "checkbox",
      "id": "enable_cascade_on_mobile",
      "label": "Enable cascade layout on mobile",
      "default": true
    },
    {
      "type": "select",
      "id": "spacing",
      "label": "Vertical space between items",
      "options": [
        {
          "value": "global",
          "label": "Same as theme settings"
        },
        {
          "value": "small",
          "label": "Small"
        },
        {
          "value": "medium",
          "label": "Medium"
        },
        {
          "value": "large",
          "label": "Large"
        }
      ],
      "default": "global"
    },
    {
      "type": "header",
      "content": "Media"
    },
    {
      "type": "paragraph",
      "content": "Adjust variation and sequence of media sizes to create unique layouts. [Learn more](https://help.switchthemes.co/)"
    },
    {
      "type": "select",
      "id": "variance",
      "label": "Variation between media sizes",
      "options": [
        {
          "value": "global",
          "label": "Same as theme settings"
        },
        {
          "value": "none",
          "label": "None"
        },
        {
          "value": "low",
          "label": "Low"
        },
        {
          "value": "medium",
          "label": "Medium"
        },
        {
          "value": "high",
          "label": "High"
        }
      ],
      "default": "global"
