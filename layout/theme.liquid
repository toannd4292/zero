{% comment %}ly_liquid_scripts_begin{% endcomment %}{% capture ly_liquid_content %}{% comment %}ly_liquid_scripts_end{% endcomment %}<!doctype html>
<html class="no-js"{% comment %}ly_dir_replace_for_[]_begin{% endcomment %}{%- assign language_code = '_' | append: request.locale.iso_code -%}{%- if '_ar_ha_he_fa_ks_ku_ps_ur_yi' contains language_code -%} dir="rtl" {%- endif -%}{% comment %}ly_dir_replace_end{% endcomment %}>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <link rel="preconnect" href="https://fonts.shopify.com" crossorigin>
    <link rel="preconnect" href="https://monorail-edge.shopifysvc.com">

    {%- if settings.favicon != blank -%}
      <link
        rel="icon"
        type="image/png"
        href="{{ settings.favicon | image_url: width: 32, height: 32 }}"
      >
    {%- endif -%}

    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}

    <title>{{ page_title }}</title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    <!-- Google Site Verification Tag -->
    <meta name="google-site-verification" content="0H2bSunpgfsjyFFwDHq5GgerRanJ5T78HMuWUv4s_rk" />

    {%- if tinyscript -%}{{ content_for_header }}{%- else -%}{% render 'tiny-script-control' %}{%- endif -%}

    {% liquid
      render 'meta-tags'
      render 'js-bridge'
    %}

    <script src="{{ 'utils.js' | asset_url }}" defer></script>
    <script
      src="{{ 'polyfills-inert.bundle.min.js' | asset_url }}"
      type="module"
    ></script>
    <script
      src="{{ 'global.bundle.min.js' | asset_url }}"
      type="module"
    ></script>
    {% if settings.quantity_style == 'dropdown' %}
      <script
        src="{{ 'quantity-dropdown.js' | asset_url }}"
        type="module"
      ></script>
    {% endif %}
    <script
      src="{{ 'quantity-stepper.js' | asset_url }}"
      type="module"
    ></script>
    {% if template == 'cart' or settings.cart_type == 'drawer' %}
      <script src="{{ 'cart-items.js' | asset_url }}" type="module"></script>
      <script src="{{ 'cart-footer.js' | asset_url }}" type="module"></script>
      {% if settings.cart_notes_enable %}
        <script src="{{ 'cart-note.js' | asset_url }}" type="module"></script>
      {% endif %}
    {% endif %}
    {% if request.design_mode %}
      {% comment %} If theme editor, load all modules {% endcomment %}
      <script
        src="{{ 'vendor-product.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'product.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'modules-video.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'vendor-store-availability.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'store-availability.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'product-model.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'sections-test-section.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'modules-background-video.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'modules-slideshow.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'theme-editor-support.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'faq.js' | asset_url }}" type="module"></script>
      <script src="{{ 'rte.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'sections-age-check.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'sections-promo-popup.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
      <script
        src="{{ 'cookies-banner.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'zoom.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'cascade-animations.js' | asset_url }}"
        type="module"
      ></script>
      <script src="{{ 'logo-list.js' | asset_url }}" type="module"></script>
      <script
        src="{{ 'gift-card-recipient.js' | asset_url }}"
        type="module"
      ></script>

      {% if settings.predictive_search_enabled %}
        <script
          src="{{ 'predictive-search.js' | asset_url }}"
          type="module"
        ></script>
      {% endif %}
    {% endif %}
    {% if settings.cart_type == 'drawer' %}
      <script
        src="{{ 'vendor-cart.bundle.min.js' | asset_url }}"
        type="module"
      ></script>
    {% endif %}
    <script src="{{ 'nav-store.js' | asset_url }}" type="module"></script>

    {{ 'base.bundle.css' | asset_url | stylesheet_tag }}
    {{ 'global.css' | asset_url | stylesheet_tag }}
    {{ 'modules-rte.bundle.css' | asset_url | stylesheet_tag }}
    {% comment %}
      <link rel="stylesheet" href="{{ 'custom.css' | asset_url }}">
    {% endcomment %}

    {% if template contains 'customers' %}
      {% comment %}theme-check-disable ParserBlockingScriptTag{% endcomment %}
      {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
      {{ 'customer_area.js' | shopify_asset_url | script_tag }}
      {% comment %}theme-check-enable ParserBlockingScriptTag{% endcomment %}
    {% endif %}

    {% render 'css-bridge' %}

    <script>
      function onYouTubeIframeAPIReady() {
        document.body.dispatchEvent(new CustomEvent('youtubeiframeapiready'));
      }
    </script>
    <script src="{{ 'enhancements.js' | asset_url }}" defer></script>
  <link rel="preload" href="{{ 'design-pack-styles.css' | asset_url }}" as="style">
{{ 'design-pack-styles.css' | asset_url | stylesheet_tag }} 
{% render 'smart-css' %} 


{% comment %}ly_dir_styles_begin{% endcomment %}{%- assign language_code = '_' | append: request.locale.iso_code -%}{%- if '_ar_ha_he_fa_ks_ku_ps_ur_yi' contains language_code -%}{{- 'ly-rtl.css' | asset_url | stylesheet_tag -}}{%- endif -%}{% comment %}ly_dir_styles_end{% endcomment %}
  </head>

<body class="theme-ctas-are-{{ settings.cta_style }}s theme-buttons-style-{{ settings.button_style }} {% if settings.show_slideshow_pagination %} show-slideshow-pagination {% endif %}">
    <a class="skip-link" href="#MainContent">
      {{- 'general.accessibility.skip_to_content' | t -}}
    </a>

    {% liquid
      sections 'header-group'
      render 'header-group-support'
    %}

    <div id="MainContent">
      {{ content_for_layout }}
    </div>


    {% liquid
      sections 'footer-group'
      sections 'overlay-group'
      render 'js-templates'
    %}

    <div
      id="screenreader-announce"
      class="sr-only"
      aria-live="polite"
      role="status"
    ></div>

    <ul hidden>
      <li id="a11y-refresh-page-message">
        {{ 'general.accessibility.refresh_page' | t }}
      </li>
      <li id="a11y-selection-message">
        {{ 'general.accessibility.selection_help' | t }}
      </li>
    </ul>

    {% comment %} Modal containers {% endcomment %}
    {% liquid
      render 'left-drawer'
      render 'right-drawer'
      render 'modal'
      render 'promo'
      render 'popup'
    %}
  
    <script src="{{ 'modals.js' | asset_url }}" type="module"></script>

    {% comment %} Modal contents {% endcomment %}
    {% if settings.cart_type == 'drawer' %}
      {% render 'drawer-cart' %}
    {% endif %}
    <script
      src="{{ 'alpine.bundle.min.js' | asset_url }}"
      type="module"
    ></script>
